{
    // velocities
    "max_vel_x": 0.1,
    "max_vel_y": 0.05,
    "max_vel_theta": 0.3,

    // networking
    "use_static_ip": true,
    "server_ip": "192.168.9.100",
    "auto_detect_players": true,
    "use_static_ip": false, 
    "max_buffer_size": 1024,
    "fake_async_time_out": 0.001, // python does not suuport aysnc, 
    				  //so a timeout is used to emulate a C-style async tcp 

    // vision
    "pos_accuracy_add": {
        // the score 'add' to the self_pos_accuracy
        // when these items appears in vision
        0: 0,       // ball
        1: 10,      // goalpost
        2: 0,       // robot
        3: 5,       // L-Intersection
        4: 5,       // T-Intersection
        5: 5        // X-Intersection
    },
    "pos_accuracy_critical": 0.0005,   // when accuracy is consider too low
    "predict_ball_pos": true,
    "predict_ball_pos_in_map": true

}
